{
    "name": "@rustresult/benchmark",
    "version": "0.0.0",
    "description": "Rust-like Result benchmark.",
    "type": "module",
    "homepage": "https://github.com/yifanwww/rustlike-result#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/rustlike-result/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/rustlike-result.git",
        "directory": "packages/benchmark"
    },
    "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
    "scripts": {
        "bench": "node dist/bench.js",
        "build": "tsc --project tsconfig.build.json",
        "clean": "rimraf --glob dist *.tsbuildinfo",
        "format": "prettier --write \"**/*.{cjs,js,mjs,json,ts,yaml,yml}\"",
        "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint .",
        "lint-fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --fix",
        "typecheck": "tsc --project tsconfig.json --noEmit --incremental false"
    },
    "dependencies": {
        "@rustresult/result": "workspace:^",
        "effect": "catalog:",
        "neverthrow": "catalog:",
        "prompts": "catalog:",
        "tinybench": "catalog:"
    },
    "devDependencies": {
        "@types/prompts": "catalog:",
        "@typescript-eslint/eslint-plugin": "catalog:",
        "@typescript-eslint/parser": "catalog:",
        "cross-env": "catalog:",
        "eslint": "catalog:",
        "eslint-config-airbnb-base": "catalog:",
        "eslint-config-prettier": "catalog:",
        "eslint-plugin-import": "catalog:",
        "eslint-plugin-jest": "catalog:",
        "eslint-plugin-prettier": "catalog:",
        "prettier": "catalog:",
        "rimraf": "catalog:",
        "typescript": "catalog:"
    }
}
