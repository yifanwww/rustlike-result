{
    "name": "@rustresult/result",
    "version": "0.6.0",
    "description": "Rust-like Result and ResultAsync for Javascript",
    "type": "module",
    "exports": {
        ".": "./dist/index.js"
    },
    "homepage": "https://github.com/yifanwww/rustlike-result#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/rustlike-result/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/rustlike-result.git",
        "directory": "packages/result"
    },
    "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
    "scripts": {
        "build": "concurrently -n js,types \"pnpm run build:js\" \"pnpm run build:types\"",
        "build:js": "tsc --project tsconfigs/tsconfig.js.json",
        "build:types": "tsc --project tsconfigs/tsconfig.types.json",
        "clean": "rimraf --glob coverage dist tsconfigs/*.tsbuildinfo",
        "format": "prettier --write \"**/*.{cjs,js,mjs,json,ts,yaml,yml}\"",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "test": "node ../../scripts/unit-test.js --watch",
        "test-full": "node ../../scripts/unit-test.js --coverage",
        "typecheck": "tsc --project tsconfig.json --noEmit --incremental false"
    },
    "devDependencies": {
        "@jest/globals": "catalog:",
        "concurrently": "catalog:",
        "eslint": "catalog:",
        "jest": "catalog:",
        "prettier": "catalog:",
        "rimraf": "catalog:",
        "typescript": "catalog:"
    },
    "files": [
        "CHANGELOG.md",
        "dist"
    ],
    "keywords": [
        "rust",
        "rustlike",
        "result",
        "rust-result",
        "rustlike-result",
        "typescript-result",
        "ts-result",
        "catch",
        "try-catch",
        "throw",
        "error",
        "error-free",
        "error-handler",
        "error-handing"
    ]
}
